<% content_for :title, "Edit account" %>
<h1>Update timestamp</h1>

  <div class="edit_info_container">
    <%= form_for @timesheet, :url => time_change_path(@timesheet.id) do |f| %>
      <%= render 'shared/error_messages' %>

      <%= f.label :punch_in %>
      <%= f.text_field :punch_in %>

      <%= f.label :punch_out %>
      <%= f.text_field :punch_out %>
    </br>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
